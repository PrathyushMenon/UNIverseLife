{
    "functions": {
      "api": {
        "runtime": "python3.8",
        "memory": 1024
      }
    },
    "routes": [
      { "handle": "filesystem", "src": "/static/(.*)" },
      { "handle": "filesystem", "src": "/templates/(.*)" },
      { "handle": "filesystem" },
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }
  