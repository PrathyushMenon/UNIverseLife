{
    "functions": {
      "api": {
        "runtime": "python3.8",
        "memory": 1024
      }
    },
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/templates/(.*)", "dest": "/templates/$1" },
      { "handle": "filesystem" },
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }
  