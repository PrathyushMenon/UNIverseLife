{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python3.8"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/templates/(.*)",
      "dest": "/templates/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
